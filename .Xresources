! rofi bullshit
 ! bg fg bgalt hlbg hlfg
! !-----------------------------------
 rofi.color-enabled: true
 !rofi.color-normal: #1f222d, #f3f4f5, #1f222d, #1f222d, #7780a1
 !rofi.color-urgent: #1f222d, #f3f4f5, #1f222d, #1f222d, #7780a1
 !rofi.color-active: #1f222d, #f3f4f5, #1f222d, #1f222d, #7780a1
 !rofi.color-window: #1f222d, #4b4f5c
 rofi.separator-style: solid

 ! special
 *.foreground:   #e8dfd6
 *.background:   #021b21
 *.cursorColor:  #e8dfd6

 ! black
 *.color0:       #032c36
 *.color8:       #065f73

 ! red
 *.color1:       #c2454e
 *.color9:       #ef5847

 ! green
 *.color2:       #7cbf9e
 *.color10:      #a2d9b1

 ! yellow
 *.color3:       #8a7a63
 *.color11:      #beb090

 ! blue
 *.color4:       #2e3340
 *.color12:      #61778d

 ! magenta
 *.color5:       #ff5879
 *.color13:      #ff99a1

 ! cyan
 *.color6:       #44b5b1
 *.color14:      #9ed9d8

 ! white
 *.color7:       #f2f1b9
 *.color15:      #f6f6c9



URxvt*geometry: 80x30
URxvt*scrollBar: false
